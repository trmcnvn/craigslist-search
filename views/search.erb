<!-- content -->
<div class="container search">
  <div class="col-sm-12">
    <h4>Search Results</h4>
    <% @results.each do |key, value| %>
      <h5><%= key %></h5>
      <ul>
        <% value.each do |item| %>
          <li>
            <span class="date js-date" data-date="<%= item[:date] %>"></span>
            <span class="title">
              <a href="<%= item[:link] %>" target="_blank">
                <%= item[:title] %>
              </a>
            </span>
          </li>
        <% end %>
      </ul>
    <% end %>
  </div>
</div>
